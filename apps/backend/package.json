{
  "name": "backend",
  "version": "1.0.0",
  "description": "Modern TypeScript Node.js Backend with Express, MySQL, MongoDB, Redis",
  "main": "dist/app.js",
  "private": true,
  "scripts": {
    "dev": "tsx watch src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "clean": "rimraf dist",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit",
    "db:init": "tsx src/scripts/initDatabase.ts",
    "db:seed": "tsx src/scripts/seed.ts",
    "db:seed:clear": "tsx src/scripts/seed.ts --clear",
    "migrate": "tsx src/migrations/cli/migrate.ts",
    "migrate:up": "tsx src/migrations/cli/migrate.ts up",
    "migrate:down": "tsx src/migrations/cli/migrate.ts down",
    "migrate:status": "tsx src/migrations/cli/migrate.ts status",
    "migrate:init": "tsx src/migrations/cli/migrate.ts init",
    "migrate:reset": "tsx src/migrations/cli/migrate.ts reset",
    "migrate:generate": "tsx src/migrations/cli/migrate.ts generate"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "commander": "^14.0.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.0",
    "express-validator": "^7.2.0",
    "helmet": "^7.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.5.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.11.0",
    "redis": "^4.7.0",
    "sequelize": "^6.37.3",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "@shared/config": "workspace:*",
    "@shared/types": "workspace:*",
    "@shared/utils": "workspace:*",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.5.0",
    "@typescript-eslint/eslint-plugin": "^8.2.0",
    "@typescript-eslint/parser": "^8.2.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "rimraf": "^5.0.10",
    "ts-jest": "^29.2.4",
    "tsx": "^4.17.0",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "typescript",
    "nodejs",
    "express",
    "mysql",
    "mongodb",
    "redis",
    "rest-api"
  ],
  "author": "Your Name",
  "license": "MIT"
}
